        <order>100</order>
        <script><![CDATA[generatePeer();

function generatePeer() {
	var gr = new GlideRecord('x_0890_devops_peer_review_task');
	gr.initialize();
	gr.newRecord();
	gr.short_description = 'Peer Review task for '+current.number;
	gr.parent = current.sys_id;
	gr.priority = current.priority;
	//gr.assignment_group = 'b6a58111db8707001e5836ee9d9619e5';
	gr.assignment_group = current.assignment_group;
	gr.description = 'Peer Review Task for '+current.number+'\n\nDevelopment Description:\n'+current.description;

	var newID = gr.insert();

	current.stage = 'Peer Review';
	var gdt = new GlideDateTime().getDisplayValue();
	current.work_notes = 'Submitted for Peer Review at '+gdt;
	current.submitted_for_peer_review = gdt;
	current.update();

	gs.addInfoMessage(gs.getMessage("Peer Review Task {0} has been created.", gr.number));

	action.setRedirectURL(current);
	action.setReturnURL(current);

}
]]></script>
